<p id="notice"><%= notice %></p>

<h1>Articles</h1>



  <tbody>
    <% @articles.each do |article| %>
    <hr/>
    <div class ="entrada">
    <h2><%= article.title %></h2>
    <h3><%= article.body %></h3>
    <p><%= article.visits_count %></p>
    <p><%= link_to 'Show', article %> - <%= link_to 'Edit', edit_article_path(article) %></p>
    </div>
     
    <% end %>
  </tbody>

<br />


